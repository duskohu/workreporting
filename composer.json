{
	"name": "nette/sandbox",
	"description": "The sandbox is a pre-packaged Nette Framework project, basic configured structure for your application.",
	"homepage": "http://nette.org",
	"type": "project",
	"license": ["BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
	"authors": [
		{
			"name": "David Grudl",
			"homepage": "http://davidgrudl.com"
		},
		{
			"name": "Nette Community",
			"homepage": "http://nette.org/contributors"
		}
	],
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.m33.cz"
        }
    ],
    "minimum-stability": "dev",
	"require": {
		"php": ">= 5.3.0",
		"nette/nette": "@dev",
        "kdyby/bootstrap-form-renderer": "@dev",
        "doctrine/annotations": "1.1",
        "kdyby/annotations": "@dev",
        "arachne/verifier": "@dev",
        "arachne/security-annotations": "@dev",
        "kdyby/html-validator-panel": "@dev",
        "kdyby/nette-session-panel": "@dev",
		"nasext/logger": "@dev",
        "stekycz/cronner": "dev-master",
        "nextras/forms": "dev-master"
	},
	"require-dev": {
		"nette/tester": "@dev"
	}
}
